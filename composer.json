{
    "name": "spryker/acl-merchant-portal",
    "type": "library",
    "description": "AclMerchantPortal module",
    "license": "proprietary",
    "require": {
        "php": ">=7.3",
        "spryker/acl": "^3.8.0",
        "spryker/acl-entity": "^0.1.0 || ^0.2.0",
        "spryker/acl-entity-extension": "^0.1.0 || ^0.2.0",
        "spryker/availability": "^9.0.0",
        "spryker/category": "^5.0.0",
        "spryker/category-image": "^1.0.0",
        "spryker/cms-block": "^3.0.0",
        "spryker/comment": "^1.0.0",
        "spryker/country": "^3.0.0",
        "spryker/currency": "^3.0.0",
        "spryker/customer": "^7.0.0",
        "spryker/event-behavior": "^1.0.0",
        "spryker/glossary": "^3.0.0",
        "spryker/kernel": "^3.30.0",
        "spryker/locale": "^3.0.0",
        "spryker/merchant": "^3.0.0",
        "spryker/merchant-category": "^1.0.0",
        "spryker/merchant-extension": "^1.1.0",
        "spryker/merchant-product": "^1.0.0",
        "spryker/merchant-profile": "^1.0.0",
        "spryker/merchant-sales-order": "^1.0.0",
