        "spryker/merchant-user-extension": "^1.0.0",
        "spryker/oms": "^11.0.0",
        "spryker/oms-product-offer-reservation": "^1.0.0",
        "spryker/payment": "^5.0.0",
        "spryker/price-product": "^4.0.0",
        "spryker/price-product-offer": "^1.0.0",
        "spryker/product": "^6.0.0",
        "spryker/product-attribute": "^1.0.0",
        "spryker/product-bundle": "^7.0.0",
        "spryker/product-category": "^4.0.0",
        "spryker/product-image": "^3.0.0",
        "spryker/product-offer": "^1.0.0",
        "spryker/product-offer-stock": "^1.0.0",
        "spryker/product-offer-validity": "^1.0.0",
        "spryker/product-search": "^5.0.0",
        "spryker/product-validity": "^1.0.0",
        "spryker/refund": "^5.0.0",
        "spryker/sales": "^11.0.0",
        "spryker/sales-configurable-bundle": "^1.0.0",
        "spryker/sales-invoice": "^1.0.0",
        "spryker/sales-order-threshold": "^1.0.0",
        "spryker/sales-product-configuration": "^0.1.0",
        "spryker/shipment": "^8.0.0",
        "spryker/state-machine": "^2.0.0",
        "spryker/stock": "^8.0.0",
        "spryker/store": "^1.0.0",
        "spryker/tax": "^5.0.0",
        "spryker/transfer": "^3.25.0",
        "spryker/url": "^3.0.0",
        "spryker/user": "^3.0.0",
        "spryker/user-password-reset": "^1.0.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/propel": "*",
        "spryker/testify": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerTest\\": "tests/SprykerTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
